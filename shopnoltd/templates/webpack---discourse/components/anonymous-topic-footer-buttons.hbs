import { precompileTemplate } from "@ember/template-compilation";
export default precompileTemplate("<div class=\"topic-footer-main-buttons\">\n  {{#each this.buttons as |button|}}\n    <DButton\n      @action={{button.action}}\n      @icon={{button.icon}}\n      @translatedLabel={{button.label}}\n      @translatedTitle={{button.title}}\n      @translatedAriaLabel={{button.ariaLabel}}\n      @disabled={{button.disabled}}\n      id={{concat \"topic-footer-button-\" button.id}}\n      class={{concatClass\n        \"btn-default\"\n        \"topic-footer-button\"\n        button.classNames\n      }}\n    />\n  {{/each}}\n  <DButton\n    @icon=\"reply\"\n    @action={{route-action \"showLogin\"}}\n    @label=\"topic.reply.title\"\n    class=\"btn-primary\"\n  />\n</div>",{ moduleName: "discourse/components/anonymous-topic-footer-buttons.hbs" })