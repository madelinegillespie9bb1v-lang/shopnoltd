      @outletArgs={{hash category=@category tag=@tag}}\n    />\n  {{/if}}\n\n  {{#if @tag}}\n    <PluginOutlet\n      @name=\"tag-navigation\"\n      @connectorTagName=\"div\"\n      @outletArgs={{hash category=@category tag=@tag}}\n    />\n  {{/if}}\n</section>",{ moduleName: "discourse/components/discovery/navigation.hbs" })