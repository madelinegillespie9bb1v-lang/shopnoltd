import { precompileTemplate } from "@ember/template-compilation";
export default precompileTemplate("<li class={{this.className}}>\n  <a\n    href={{this.linkHref}}\n    title={{this.linkTitle}}\n    {{on \"click\" this.onClick}}\n  >\n    {{#if this.iconComponent}}\n      <this.iconComponent @data={{this.iconComponentArgs}} />\n    {{else}}\n      {{d-icon this.icon}}\n    {{/if}}\n    <div>\n      {{#if this.label}}\n        <span class={{concat \"item-label \" this.labelClass}}>\n          {{this.label}}\n        </span>\n      {{/if}}\n      {{#if this.description}}\n        <span\n          class={{concat \"item-description \" this.descriptionClass}}\n          data-topic-id={{this.topicId}}\n        >\n          {{this.description}}\n        </span>\n      {{/if}}\n    </div>\n\n    {{#if this.endComponent}}\n      <this.endComponent />\n    {{/if}}\n  </a>\n  <PluginOutlet @name=\"menu-item-end\" @outletArgs={{this.endOutletArgs}} />\n</li>",{ moduleName: "discourse/components/user-menu/menu-item.hbs" })