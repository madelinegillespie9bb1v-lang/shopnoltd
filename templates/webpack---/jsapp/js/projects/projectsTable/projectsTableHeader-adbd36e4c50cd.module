// when also in a highlighted table row\n$kobo-gray-300: hsl(227deg 18% 90%); // #e1e3ea\n\n// default light background, icons on hover, sidebar menu, table headings, …\n$kobo-gray-200: hsl(228deg 16% 94%); // #edeef2\n\n// table row highlight on hover\n$kobo-gray-100: hsl(240deg 20% 98%); // #f9f9fb\n\n// white background\n$kobo-white: hsl(0deg 0% 100%); // #ffffff\n\n/*\n * Primary colours & shades\n */\n\n$kobo-dark-blue: hsl(207deg 77% 32%); // #135991\n$kobo-hover-blue: hsl(207deg 77% 43%); // #1977c2\n$kobo-blue: hsl(207deg 90% 54%); // #2095f3\n$kobo-mid-blue: hsl(207deg 90% 77%); // #8fcaf9 (previously $kobo-alt-blue)\n$kobo-light-blue: hsl(207deg 88% 91%); // #d2e9fc (previously $kobo-mid-blue)\n$kobo-bg-blue: hsl(206deg 84% 95%); // #e8f4fd (previously $kobo-light-blue)\n\n$kobo-dark-teal: hsl(185deg 57% 25%); // #1b5e64\n$kobo-hover-teal: hsl(185deg 57% 35%); // #26838c\n$kobo-teal: hsl(185deg 57% 57%); // #52c5d0\n$kobo-mid-teal: hsl(186deg 57% 75%); // #9bdde4\n$kobo-light-teal: hsl(185deg 58% 85%); // #c3ebef\n$kobo-bg-teal: hsl(188deg 60% 95%); // #ebf8fa\n\n/*\n * Secondary & tertiary colors\n */\n\n$kobo-storm: hsl(224deg 33% 59%); // #7386b9\n$kobo-mid-storm: hsl(223deg 34% 79%); // #b8c2dc\n$kobo-light-storm: hsl(225deg 31% 95%); // #eef0f6 (previously $kobo-cloud)\n\n$kobo-purple: hsl(290deg 53% 65%); // #c577d5\n$kobo-violet: hsl(290deg 53% 85%); // #e6c5ed\n$kobo-pink: hsl(290deg 53% 93%); // #f4e5f7\n\n$kobo-dark-red: hsl(0deg 100% 26%); // #850000\n$kobo-red: hsl(0deg 100% 31%); // #9d0000 (previously $kobo-dark-red)\n$kobo-mid-red: hsl(0deg 100% 75%); // #ff8080 (previously $kobo-red)\n$kobo-hover-red: hsl(0deg 100% 90%); // #ffcccc\n$kobo-light-red: hsl(0deg 100% 96%); // #ffe9e9\n\n$kobo-dark-amber: hsl(30deg 100% 25%); // #803f00\n$kobo-amber: hsl(29deg 100% 75%); // #ffbe80\n$kobo-light-amber: hsl(30deg 100% 90%); // #ffe8cc\n\n$kobo-dark-green: hsl(146deg 71% 31%); // #178949\n$kobo-green: hsl(146deg 72% 75%); // #92edba\n$kobo-light-green: hsl(147deg 69% 95%); // #e3f9db (previously $kobo-lime)\n","@use 'sass:color';\n@use 'scss/mixins';\n@use 'scss/sizes';\n@use 'scss/colors';\n@use 'scss/variables';\n@use 'scss/z-indexes';\n\n$projects-table-hover-bg: colors.$kobo-gray-100;\n$projects-table-row-padding: sizes.$x8;\n// The shared height of a row: project rows are bigger due to padding, but\n// header rows use this size without paddings\n$projects-table-row-height: sizes.$x36;\n\n.row {\n  display: flex;\n  flex-direction: row;\n  position: relative;\n  align-items: stretch;\n  cursor: pointer;\n  margin: 0 sizes.$x8;\n\n  user-select: none;\n}\n\n.cell {\n  border-bottom: sizes.$x1 solid colors.$kobo-gray-300;\n  padding: $projects-table-row-padding;\n  color: colors.$kobo-gray-700;\n  flex: 1 0 auto;\n  // ensures the table columns are same width for all rows\n  width: 10%;\n\n  > * {\n    cursor: inherit;\n  }\n\n  :global label {\n    cursor: inherit;\n  }\n\n  &[disabled] {\n    pointer-events: none;\n  }\n\n  &[data-field='checkbox'] {\n    max-width: $projects-table-row-padding + sizes.$x20 + $projects-table-row-padding;\n    width: $projects-table-row-padding + sizes.$x20 + $projects-table-row-padding;\n    @include mixins.centerRowFlex;\n  }\n\n  &[data-field='countries'],\n  &[data-field='description'],\n  &[data-field='ownerEmail'],\n  &[data-field='ownerOrganization'],\n  &[data-field='status'] {\n    width: 13%;\n  }\n\n  &[data-field='submissions'] {\n    width: 10%;\n  }\n\n  &[data-field='countries'] {\n    width: 15%;\n\n    > *:not(:last-child) {\n      margin-right: sizes.$x5;\n    }\n  }\n\n  &[data-field='ownerFullName'],\n  &[data-field='ownerUsername'] {\n    width: 10%;\n    font-weight: 600;\n  }\n\n  &[data-field='dateModified'],\n  &[data-field='dateDeployed'] {\n    width: 14%;\n  }\n\n  &[data-field='languages'] {\n    width: 15%;\n  }\n\n  &[data-field='name'] {\n    color: colors.$kobo-dark-blue;\n    font-weight: 600;\n    width: 30%;\n\n    // Handling the fact that the cell content is a link\n    padding: 0;\n    :global a {\n      @include mixins.textEllipsis;\n      color: inherit;\n      display: block;\n      padding: $projects-table-row-padding;\n\n      // Keyboard-focus ring for links\n      &:focus-visible .asset-name {\n        outline: 1px solid colors.$kobo-blue;\n        border-radius: 2px;\n        outline-offset: 4px;\n        text-decoration-color: colors.$kobo-mid-blue;\n        text-decoration-line: underline;\n      }\n    }\n\n    // Setup for underlining the Project Name on row hover.\n    // This clarifies the size of the checkbox touch-target\n    // and makes full-navigation a little more predictable.\n    text-decoration: 1px solid underline color.change(colors.$kobo-mid-blue, $alpha: 0);\n    text-underline-offset: 3px;\n  }\n\n  &[data-field='sector'] {\n    width: 15%;\n  }\n}\n\n.row.rowTypeProject {\n  .cell {\n    @include mixins.textEllipsis;\n    line-height: $projects-table-row-height;\n\n    // performance hints for resizing\n    contain: inline-size layout paint style;\n\n    // don't show ellipsis by badges\n    &[data-field='status'],\n    &[data-field='countries'],\n    &[data-field='submissions'] {\n      text-overflow: clip;\n    }\n\n    // To make a more forgiving checkbox interaction, make this cell\n    // much wider than it otherwise would be.\n    &[data-field='checkbox'] {\n      position: relative;\n      line-height: unset;\n      // create hanging pseudo-elements to expand touch targets\n      &::after,\n      &::before {\n        display: inline-block;\n        content: '';\n        position: absolute;\n        top: 0;\n        bottom: 0;\n        z-index: 2;\n      }\n      // right: cover whitespace up to the project name link\n      &::after {\n        right: 0 - $projects-table-row-padding;\n        width: $projects-table-row-padding;\n      }\n      // left: cover whitespace left of project table\n      &::before {\n        left: 0 - sizes.$x32;\n        width: sizes.$x32;\n      }\n      // uncrop element\n      overflow: visible;\n      contain: inline-size style;\n    }\n  }\n\n  &:hover .cell {\n    background-color: $projects-table-hover-bg;\n\n    &.cellHighlighted {\n      background-color: color.change(colors.$kobo-bg-blue, $alpha: 0.6);\n    }\n\n    // Various hover rules to tell the user what will happen:\n    // - click project cell  ->  navigate to project\n    // - click other cell    ->  toggle the checkbox for project\n\n    // Hovering any cell  ->  Outline the checkbox\n    &[data-field='checkbox'] {\n      :global .checkbox__input {\n        outline: sizes.$x2 solid colors.$kobo-mid-blue;\n        outline-offset: 0 - sizes.$x1;\n      }\n\n      &:hover {\n        // Do an even bigger outline if you're actually near the checkbox\n        :global .checkbox__input {\n          outline-width: sizes.$x3;\n          outline-offset: 0 - sizes.$x1;\n\n          // Override checkbox hover states to reduce flashing\n          &:hover {\n            background-color: colors.$kobo-white;\n\n            &:checked {\n              background-color: colors.$kobo-blue;\n            }\n          }\n        }\n      }\n    }\n    // Specificity: Keep the keyboard focus styles, despite the pointer\n    &[data-field='checkbox'] :global .checkbox__input:focus-visible,\n    &[data-field='checkbox']:hover :global .checkbox__input:focus-visible {\n      @include mixins.default-ui-focus;\n    }\n\n    // Hovering 'Project name' cell…\n    //  ->  Underline project name (link)\n    &[data-field='name']:hover {\n      text-decoration-color: colors.$kobo-mid-blue;\n    }\n    //  ->  De-emphasize the checkbox\n    &[data-field='checkbox']:has(~ [data-field='name']:hover) {\n      //  (Uses :has(), so not supported in Firefox yet)\n      :global .checkbox__input {\n        outline: none;\n      }\n    }\n  }\n}\n\n.row.rowTypeHeader {\n  .cell {\n    @include mixins.centerRowFlex;\n    color: colors.$kobo-gray-700;\n    border-top: 0;\n    cursor: pointer;\n    height: $projects-table-row-height;\n    padding: 0; // dropdown does the padding\n    background-color: colors.$kobo-white;\n\n    // performance hints for resizing\n    contain: inline-size style;\n\n    &:hover {\n      color: colors.$kobo-gray-900 !important;\n    }\n\n    // checkbox header not clickable - use default cursor\n    &[data-field='checkbox'] {\n      cursor: default;\n    }\n  }\n}\n\n.row.rowTypeMessage {\n  padding: sizes.$x40;\n  font-size: 1.2rem;\n  justify-content: center;\n  text-align: center;\n}\n\n.headerLabel {\n  font-weight: 700;\n  font-size: sizes.$x13;\n}\n\n// Highlighted columns:\n\n.row.rowTypeProject .cell.cellHighlighted {\n  background-color: color.change(colors.$kobo-bg-blue, $alpha: 0.3);\n}\n\n.row.rowTypeHeader .cell.cellHighlighted {\n  background-color: colors.$kobo-bg-blue;\n  color: colors.$kobo-dark-blue;\n}\n","/*\n * A temporary-ish file for defining all the sizes we use in the app to avoid\n * creating unnecessary new ones. Ultimately we would like to have a small\n * nice palette and start merging similar ones. This will also help us find\n * how many of each of them we use.\n */\n@use 'sass:math';\n\n// absolute sizes\n$x1: 1px;\n$x2: 2px;\n$x3: 3px;\n$x4: 4px;\n$x5: 5px;\n$x6: 6px;\n$x8: 8px;\n$x10: 10px;\n$x12: 12px;\n$x13: 13px;\n$x14: 14px;\n$x15: 15px;\n$x16: 16px;\n$x18: 18px;\n$x20: 20px;\n$x21: 21px;\n$x22: 22px;\n$x24: 24px;\n$x28: 28px;\n$x30: 30px;\n$x32: 32px;\n$x36: 36px;\n$x38: 38px;\n$x40: 40px;\n$x48: 48px;\n$x50: 50px;\n$x60: 60px;\n$x80: 80px;\n$x100: 100px;\n$x120: 120px;\n$x140: 140px;\n$x150: 150px;\n$x180: 180px;\n$x200: 200px;\n$x300: 300px;\n$x350: 350px;\n$x400: 400px;\n$x600: 600px;\n$x800: 800px;\n\n// takes a pixel dimension and returns it in rem\n@function pxToRem($size) {\n  $remSize: math.div($size, 16px);\n  @return #{$remSize}rem;\n}\n\n// relative sizes (rem units - scales with root document font size)\n$r12: pxToRem($x12);\n$r14: pxToRem($x14);\n$r16: pxToRem($x16); // 1em at default font size of 16px\n$r18: pxToRem($x18);\n\n","@use 'sass:color';\n@use 'scss/sizes';\n@use 'scss/colors';\n\n@mixin buttonReset() {\n  display: inline-block;\n  vertical-align: middle;\n  position: relative; // needed for tooltips etc.\n  appearance: none;\n  cursor: pointer;\n  text-decoration: none;\n  color: inherit;\n  background-color: transparent;\n  height: auto;\n  padding: 0;\n  margin: 0;\n  border: none;\n\n  &[disabled] {\n    pointer-events: none;\n    opacity: 0.5;\n  }\n\n  &:active {\n    // to make it look pressed-in\n    transform: translateY(sizes.$x1);\n  }\n}\n\n@mixin centerRowFlex() {\n  display: flex;\n  flex-direction: row;\n  align-content: center;\n  align-items: center;\n}\n\n@mixin textEllipsis() {\n  overflow: hidden;\n  white-space: nowrap;\n  text-overflow: ellipsis;\n}\n\n// Use this for dropdowns and menus\n@mixin floatingRoundedBox() {\n  background-color: colors.$kobo-white;\n  border-radius: sizes.$x6;\n  box-shadow: 0 0 sizes.$x6 color.change(colors.$kobo-storm, $alpha: 0.3);\n}\n\n@mixin default-ui-focus() {\n  outline: sizes.$x3 solid colors.$kobo-mid-blue !important;\n  border-color: colors.$kobo-blue !important;\n}\n","@use 'sass:color';\n@use 'sass:math';\n@use 'scss/mixins';\n@use 'scss/sizes';\n@use 'scss/colors';\n@use 'js/components/common/icon';\n@use './projectsTableRow.module';\n\n// This file contains all the styles that are being used exclusively in\n// `ProjectsTableHeader` component. Most of styles it is using are coming\n// from `projectsTableRow.module.scss` file.\n\n// Column Resizer handle styles are here, too.\n\n.columnRoot {\n  // For column width resizers\n  position: relative; // Be a positioned ancestor for resize handles\n  user-select: none;  // Prevent accidental text selection\n\n  // Make the whole header cell clickable for the dropdown trigger.\n  :global .kobo-dropdown,\n  :global .kobo-dropdown__trigger {\n    position: absolute;\n    top: 0;\n    right: 0;\n    bottom: 0;\n    left: 0;\n  }\n}\n\n.trigger {\n  @include mixins.centerRowFlex;\n\n  padding-top: 0;\n  padding-bottom: 0;\n  padding-right: projectsTableRow.$projects-table-row-padding;\n  // 10 px is a magic number that puts the caret icon in the same line as the\n  // content of the cells below\n  padding-left: projectsTableRow.$projects-table-row-padding + sizes.$x10;\n\n  cursor: pointer;\n  line-height: projectsTableRow.$projects-table-row-height;\n\n  :global {\n    // Dropdown arrow\n    .k-icon:first-child {\n      // Pull the dropdown label and arrow to the left somewhat.\n      // This measurement isn't scientific, it's just supposed to look good.\n      margin-left: 0 - icon.$s-icon-xxs - sizes.$x2;\n      padding-right: sizes.$x3;\n      opacity: 0.6;\n    }\n\n    // Dropdown Label\n    label {\n      @include mixins.textEllipsis;\n    }\n\n    // Sorting indicator\n    .k-icon:not(:first-child) {\n      margin-left: sizes.$x6;\n      color: colors.$kobo-teal; // make it stand out\n    }\n  }\n}\n\n.isMenuVisible .trigger {\n  color: colors.$kobo-blue;\n}\n\n.dropdownContent {\n  @include mixins.floatingRoundedBox;\n  padding: sizes.$x10;\n  min-width: sizes.$x120;\n\n  :global {\n    .k-button {\n      width: 100%;\n      white-space: nowrap;\n\n      &:not(:first-child) {\n        margin-top: sizes.$x6;\n      }\n    }\n  }\n}\n\n// Resizers\n\n$line-width: sizes.$x1;\n// Resizer \"pad\" should be at least 10px wide for ease of use.\n// If $line-width is odd, $resizer-width should be odd, too.\n$resizer-width: sizes.$x16 + $line-width;\n\n// Resizer \"pad\" - interactive area\n.resizer {\n  cursor: ew-resize; // col-resize <-|->   ew-resize <->\n  border-radius: sizes.$x4;\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  right: math.div($resizer-width - $line-width, -2);\n  width: $resizer-width;\n  z-index: 2; // Overlap adjacent column header and cell width indicator\n  background-color: colors.$kobo-bg-blue; // Resizer \"pad\" color\n  opacity: 0;\n\n  &:hover {\n    opacity: 1;\n  }\n  // performance hints\n  contain: inline-size layout paint style;\n}\n\n// Active resizer \"line\" - visible when hovering\n.resizer::after {\n  display: inline-block;\n  content: '';\n  position: absolute;\n  top: sizes.$x4; // $resizer-line-vertical-offset;\n  bottom: sizes.$x4;\n  right: math.div($resizer-width - $line-width, 2);\n  width: $line-width;\n  background-color: colors.$kobo-blue;\n  opacity: 0;\n  z-index: 2; // Layer above gray lines and above adjacent column header\n}\n\n.resizer:hover::after {\n  opacity: 1;\n}\n\n// Thin gray vertical lines\n// Appear on either side of the header cell, indicating\n// the width of the cell you're going to resize.\n.columnRoot,\n.columnRoot:hover {\n  &::before,\n  &::after {\n    top: sizes.$x6;\n    bottom: sizes.$x6;\n    display: inline-block;\n    position: absolute;\n    background-color: colors.$kobo-gray-500;\n    // Give a little fade\n    border-top: sizes.$x1 solid colors.$kobo-gray-300;\n    border-bottom: sizes.$x1 solid colors.$kobo-gray-300;\n    box-shadow:\n      0 -1px 0 colors.$kobo-gray-200,\n      0  1px 0 colors.$kobo-gray-200;\n    width: $line-width;\n    content: '';\n    z-index: 1; // Layer below the resizer \"pad\" but above adjacent column headers\n    pointer-events: none;\n    transition: opacity 0.2s;\n  }\n\n  &::before {\n    left: (-$line-width);\n  }\n\n  &::after {\n    right: 0;\n  }\n}\n\n.columnRoot::before,\n.columnRoot::after {\n  opacity: 0;\n}\n\n.columnRoot:hover::before,\n.columnRoot:hover::after {\n  opacity: 1;\n}\n// Always hide leftmost width indicator\n// nth-child(2) because of checkbox column\n.columnRoot:nth-child(2)::before {\n  display: none;\n}\n"],"sourceRoot":""}]);
