import { precompileTemplate } from "@ember/template-compilation";
export default precompileTemplate("<a\n  role=\"tab\"\n  class={{this.classNames}}\n  id={{this.id}}\n  tabindex={{this.tabIndex}}\n  title={{@tab.title}}\n  aria-label={{@tab.title}}\n  aria-selected={{if this.isActive \"true\" \"false\"}}\n  aria-controls={{this.ariaControls}}\n  data-tab-number={{@tab.position}}\n  href={{@tab.linkWhenActive}}\n  {{on \"click\" @onTabClick}}\n  {{on \"keydown\" @onTabClick}}\n>\n  {{d-icon @tab.icon}}\n  {{#if @tab.count}}\n    <span aria-hidden=\"true\" class=\"badge-notification\">{{@tab.count}}</span>\n  {{/if}}\n  {{yield}}\n</a>",{ moduleName: "discourse/components/user-menu/menu-tab.hbs" })