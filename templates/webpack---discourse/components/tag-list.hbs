import { precompileTemplate } from "@ember/template-compilation";
export default precompileTemplate("{{#if this.title}}\n  <h3>{{this.title}}</h3>\n{{/if}}\n{{#if this.category}}\n  <CategoryTitleLink @category={{this.category}} />\n{{/if}}\n{{#if this.tagGroupName}}\n  <h3>{{this.tagGroupName}}</h3>\n{{/if}}\n{{#each this.sortedTags as |tag|}}\n  <div class=\"tag-box\">\n    {{discourse-tag\n      tag.id\n      description=tag.description\n      isPrivateMessage=this.isPrivateMessage\n      pmOnly=tag.pmOnly\n      tagsForUser=this.tagsForUser\n    }}\n    {{#if tag.pmOnly}}\n      {{d-icon \"envelope\"}}\n    {{/if}}\n    {{#if tag.totalCount}}\n      <span class=\"tag-count\">\n        x\n        {{tag.totalCount}}\n      </span>\n    {{/if}}\n  </div>\n{{/each}}\n<div class=\"clearfix\"></div>",{ moduleName: "discourse/components/tag-list.hbs" })