import { precompileTemplate } from "@ember/template-compilation";
export default precompileTemplate("<label for=\"choose-message-title\">{{i18n \"choose_message.title.search\"}}</label>\n\n<TextField\n  @value={{this.messageTitle}}\n  @placeholderKey=\"choose_message.title.placeholder\"\n  @id=\"choose-message-title\"\n/>\n\n{{#if this.loading}}\n  <p>{{i18n \"loading\"}}</p>\n{{else}}\n  {{#if this.noResults}}\n    <p>{{i18n \"choose_message.none_found\"}}</p>\n  {{else}}\n    {{#each this.messages as |m|}}\n      <div class=\"controls existing-message\">\n        <label class=\"radio\">\n          <input\n            id=\"choose-message-{{m.id}}\"\n            {{on \"click\" (fn this.chooseMessage m)}}\n            type=\"radio\"\n            name=\"choose_message_id\"\n          />\n          <span class=\"message-title\">\n            {{m.title}}\n          </span>\n        </label>\n      </div>\n    {{/each}}\n  {{/if}}\n{{/if}}",{ moduleName: "discourse/components/choose-message.hbs" })