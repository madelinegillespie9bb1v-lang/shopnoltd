import { precompileTemplate } from "@ember/template-compilation";
export default precompileTemplate("<div role=\"heading\" aria-level=\"2\" class=\"table-heading\">\n  {{i18n (concat \"filters.\" this.filter \".title\")}}\n  <PluginOutlet\n    @name=\"categories-topics-table-heading\"\n    @connectorTagName=\"div\"\n  />\n</div>\n\n{{#if this.topics}}\n  {{#each this.topics as |t|}}\n    {{#if this.currentUser.use_glimmer_topic_list}}\n      <TopicList::LatestTopicListItem @topic={{t}} />\n    {{else}}\n      <LatestTopicListItem @topic={{t}} />\n    {{/if}}\n  {{/each}}\n\n  <div class=\"more-topics\">\n    {{#if\n      (eq\n        this.siteSettings.desktop_category_page_style\n        \"categories_and_latest_topics_created_date\"\n      )\n    }}\n      <a\n        href={{get-url (concat \"/\" this.filter \"?order=created\")}}\n        class=\"btn btn-default pull-right\"\n      >{{i18n \"more\"}}</a>\n    {{else}}\n      <a\n        href={{get-url (concat \"/\" this.filter)}}\n        class=\"btn btn-default pull-right\"\n      >{{i18n \"more\"}}</a>\n    {{/if}}\n  </div>\n{{else}}\n  <div class=\"no-topics\">\n    <h3>{{i18n (concat \"topics.none.\" this.filter)}}</h3>\n  </div>\n{{/if}}",{ moduleName: "discourse/components/categories-topic-list.hbs" })