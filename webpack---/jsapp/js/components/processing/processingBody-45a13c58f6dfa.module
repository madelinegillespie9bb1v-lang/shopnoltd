/ AnalysisHeader handles the top padding\n  padding-top: 0;\n}\n\n// The content of these steps is different.\n.root.stepBegin,\n.root.stepConfig {\n  max-width: sizes.$x600 + (2 * sizes.$x30); // include paddings\n  margin: 0 auto;\n}\n\n.root.stepConfig :global .loadingSpinner {\n  height: auto;\n}\n\n.root.stepBegin {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.root :global h2 {\n  font-size: sizes.$x16;\n  font-weight: 400;\n  text-align: center;\n}\n\n.root :global p {\n  text-align: center;\n  line-height: 1.6;\n}\n\n.root :global h2,\n.root :global p {\n  margin: 0;\n\n  &:not(:last-child) {\n    margin-bottom: sizes.$x20;\n  }\n}\n\n.root :global .region-selector {\n  margin: 0;\n\n  &:not(:last-child) {\n    margin-bottom: sizes.$x40;\n  }\n}\n\n.text {\n  white-space: pre-wrap;\n  border-top: sizes.$x2 solid colors.$kobo-gray-200;\n  padding-top: sizes.$x16;\n  width: 100%;\n  overflow-y: auto;\n  flex: 1;\n}\n\n.textarea {\n  border: sizes.$x2 solid colors.$kobo-gray-200;\n  border-radius: sizes.$x6;\n  padding: sizes.$x16;\n  resize: none;\n  width: 100%;\n  flex: 1;\n}\n\n.header {\n  font-weight: 400;\n  text-align: center;\n  margin-bottom: sizes.$x24;\n}\n\n.header:not(:first-child) {\n  margin-top: sizes.$x32;\n}\n\n.footer {\n  @include mixins.centerRowFlex;\n\n  padding: sizes.$x20 0;\n  justify-content: space-between;\n  gap: sizes.$x14;\n}\n\n.footerRightButtons {\n  @include mixins.centerRowFlex;\n\n  gap: sizes.$x14;\n}\n\n.footerCenterButtons {\n  @include mixins.centerRowFlex;\n\n  flex: 1;\n  justify-content: center;\n  gap: sizes.$x14;\n}\n\n/*\n * transxHeader\n */\n\n.transxHeader {\n  @include mixins.centerRowFlex;\n  margin-bottom: sizes.$x12;\n  gap: 8px;\n}\n\n.transxHeaderButtons {\n  @include mixins.centerRowFlex;\n\n  flex: 1;\n  justify-content: flex-end;\n  gap: inherit;\n}\n\n.transxHeaderLanguage {\n  font-weight: 700;\n  // With single column layout this can get very long or very short\n  max-width: 30%;\n\n  &:not(:first-child) {\n    margin-left: sizes.$x4;\n  }\n}\n\n.transxHeaderDate {\n  @include mixins.textEllipsis;\n\n  background-color: colors.$kobo-light-storm;\n  height: sizes.$x28;\n  line-height: sizes.$x28;\n  border-radius: sizes.$x14;\n  padding: 0 sizes.$x14;\n}\n\n@include breakpoints.breakpoint(veryNarrow) {\n  .root {padding: 20px;}\n\n  .root.stepBegin,\n  .root.stepConfig {\n    padding-top: sizes.$x30;\n  }\n\n  .header {\n    font-size: 16px;\n    line-height: 18px;\n  }\n\n  .transxHeaderDateLong {display: none;}\n}\n\n@include breakpoints.breakpoint(narrowAndUp) {\n  .root {padding: 30px;}\n\n  .root.stepBegin,\n  .root.stepConfig {\n    padding-top: sizes.$x50;\n  }\n\n  .header {\n    font-size: 18px;\n    line-height: 20px;\n  }\n\n  .transxHeaderDateShort {display: none;}\n}\n\n@include breakpoints.breakpoint(medium) {\n  .transxHeader {\n    gap: 12px;\n  }\n\n  .transxHeaderLanguage {\n    // cramped two columns layout - this value makes sure we don't overflow\n    // horizontally\n    max-width: 140px;\n  }\n}\n\n@include breakpoints.breakpoint(wide) {\n  .transxHeaderLanguage {\n    max-width: 140px;\n  }\n}\n\n@include breakpoints.breakpoint(wideAndUp) {\n  .transxHeader {\n    gap: 20px;\n  }\n}\n\n@include breakpoints.breakpoint(veryWide) {\n  .transxHeaderLanguage {\n    max-width: 250px;\n  }\n}\n","/*\n * A temporary-ish file for defining all the sizes we use in the app to avoid\n * creating unnecessary new ones. Ultimately we would like to have a small\n * nice palette and start merging similar ones. This will also help us find\n * how many of each of them we use.\n */\n@use 'sass:math';\n\n// absolute sizes\n$x1: 1px;\n$x2: 2px;\n$x3: 3px;\n$x4: 4px;\n$x5: 5px;\n$x6: 6px;\n$x8: 8px;\n$x10: 10px;\n$x12: 12px;\n$x13: 13px;\n$x14: 14px;\n$x15: 15px;\n$x16: 16px;\n$x18: 18px;\n$x20: 20px;\n$x21: 21px;\n$x22: 22px;\n$x24: 24px;\n$x28: 28px;\n$x30: 30px;\n$x32: 32px;\n$x36: 36px;\n$x38: 38px;\n$x40: 40px;\n$x48: 48px;\n$x50: 50px;\n$x60: 60px;\n$x80: 80px;\n$x100: 100px;\n$x120: 120px;\n$x140: 140px;\n$x150: 150px;\n$x180: 180px;\n$x200: 200px;\n$x300: 300px;\n$x350: 350px;\n$x400: 400px;\n$x600: 600px;\n$x800: 800px;\n\n// takes a pixel dimension and returns it in rem\n@function pxToRem($size) {\n  $remSize: math.div($size, 16px);\n  @return #{$remSize}rem;\n}\n\n// relative sizes (rem units - scales with root document font size)\n$r12: pxToRem($x12);\n$r14: pxToRem($x14);\n$r16: pxToRem($x16); // 1em at default font size of 16px\n$r18: pxToRem($x18);\n\n","@use 'sass:color';\n@use 'scss/sizes';\n@use 'scss/colors';\n\n@mixin buttonReset() {\n  display: inline-block;\n  vertical-align: middle;\n  position: relative; // needed for tooltips etc.\n  appearance: none;\n  cursor: pointer;\n  text-decoration: none;\n  color: inherit;\n  background-color: transparent;\n  height: auto;\n  padding: 0;\n  margin: 0;\n  border: none;\n\n  &[disabled] {\n    pointer-events: none;\n    opacity: 0.5;\n  }\n\n  &:active {\n    // to make it look pressed-in\n    transform: translateY(sizes.$x1);\n  }\n}\n\n@mixin centerRowFlex() {\n  display: flex;\n  flex-direction: row;\n  align-content: center;\n  align-items: center;\n}\n\n@mixin textEllipsis() {\n  overflow: hidden;\n  white-space: nowrap;\n  text-overflow: ellipsis;\n}\n\n// Use this for dropdowns and menus\n@mixin floatingRoundedBox() {\n  background-color: colors.$kobo-white;\n  border-radius: sizes.$x6;\n  box-shadow: 0 0 sizes.$x6 color.change(colors.$kobo-storm, $alpha: 0.3);\n}\n\n@mixin default-ui-focus() {\n  outline: sizes.$x3 solid colors.$kobo-mid-blue !important;\n  border-color: colors.$kobo-blue !important;\n}\n","// Kobo global palette of colors.\n\n/*\n * The Kobo neutral scale\n */\n\n// (accessible) our black, very very rare, only use if necessary for contrast\n$kobo-gray-900: hsl(225deg 17% 14%); // #1e212a\n\n// (accessible) active text, important text, text on background, top navigation\n// background\n$kobo-gray-800: hsl(225deg 16% 24%); // #333847\n\n// (accessible) default text, inactive text, options text, buttons text,\n// tabs text, notifications background, hover for popups and dropdowns\n$kobo-gray-700: hsl(226deg 16% 42%); // #5a627d\n\n// (readable) clickable icons on hover\n$kobo-gray-600: hsl(225deg 16% 58%); // #828ba5\n\n// icons default (clickable), arrows and indicators default\n$kobo-gray-500: hsl(226deg 16% 70%); // #a6acbf\n\n// inactive icons, some decorative icons\n$kobo-gray-400: hsl(228deg 16% 82%); // #cacdd9\n\n// scroll bars, table lines, dividers, background of clickable icons on hover\n// when also in a highlighted table row\n$kobo-gray-300: hsl(227deg 18% 90%); // #e1e3ea\n\n// default light background, icons on hover, sidebar menu, table headings, …\n$kobo-gray-200: hsl(228deg 16% 94%); // #edeef2\n\n// table row highlight on hover\n$kobo-gray-100: hsl(240deg 20% 98%); // #f9f9fb\n\n// white background\n$kobo-white: hsl(0deg 0% 100%); // #ffffff\n\n/*\n * Primary colours & shades\n */\n\n$kobo-dark-blue: hsl(207deg 77% 32%); // #135991\n$kobo-hover-blue: hsl(207deg 77% 43%); // #1977c2\n$kobo-blue: hsl(207deg 90% 54%); // #2095f3\n$kobo-mid-blue: hsl(207deg 90% 77%); // #8fcaf9 (previously $kobo-alt-blue)\n$kobo-light-blue: hsl(207deg 88% 91%); // #d2e9fc (previously $kobo-mid-blue)\n$kobo-bg-blue: hsl(206deg 84% 95%); // #e8f4fd (previously $kobo-light-blue)\n\n$kobo-dark-teal: hsl(185deg 57% 25%); // #1b5e64\n$kobo-hover-teal: hsl(185deg 57% 35%); // #26838c\n$kobo-teal: hsl(185deg 57% 57%); // #52c5d0\n$kobo-mid-teal: hsl(186deg 57% 75%); // #9bdde4\n$kobo-light-teal: hsl(185deg 58% 85%); // #c3ebef\n$kobo-bg-teal: hsl(188deg 60% 95%); // #ebf8fa\n\n/*\n * Secondary & tertiary colors\n */\n\n$kobo-storm: hsl(224deg 33% 59%); // #7386b9\n$kobo-mid-storm: hsl(223deg 34% 79%); // #b8c2dc\n$kobo-light-storm: hsl(225deg 31% 95%); // #eef0f6 (previously $kobo-cloud)\n\n$kobo-purple: hsl(290deg 53% 65%); // #c577d5\n$kobo-violet: hsl(290deg 53% 85%); // #e6c5ed\n$kobo-pink: hsl(290deg 53% 93%); // #f4e5f7\n\n$kobo-dark-red: hsl(0deg 100% 26%); // #850000\n$kobo-red: hsl(0deg 100% 31%); // #9d0000 (previously $kobo-dark-red)\n$kobo-mid-red: hsl(0deg 100% 75%); // #ff8080 (previously $kobo-red)\n$kobo-hover-red: hsl(0deg 100% 90%); // #ffcccc\n$kobo-light-red: hsl(0deg 100% 96%); // #ffe9e9\n\n$kobo-dark-amber: hsl(30deg 100% 25%); // #803f00\n$kobo-amber: hsl(29deg 100% 75%); // #ffbe80\n$kobo-light-amber: hsl(30deg 100% 90%); // #ffe8cc\n\n$kobo-dark-green: hsl(146deg 71% 31%); // #178949\n$kobo-green: hsl(146deg 72% 75%); // #92edba\n$kobo-light-green: hsl(147deg 69% 95%); // #e3f9db (previously $kobo-lime)\n","/*\n * Responsive breakpoints for usage in media queries\n */\n$b480: 480px;\n$b600: 600px;\n// 768 is a legacy breakpoint. We use it (in old code mostly), and we might or\n// might not want to use it in the future (TBD).\n$b768: 768px;\n$b960: 960px;\n$b1140: 1140px;\n$b1440: 1440px;\n$bMobileMenu: $b1140;\n\n/*\n\nThe names for screen sizes and the breakpoints that divides them are:\n\n~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n~  |              ↔︎            ↔︎            ↔︎            ↔︎              |  ~\n~  |  veryNarrow  ↔︎   narrow   ↔︎   medium   ↔︎    wide    ↔︎   veryWide   |  ~\n~  |              ↔︎            ↔︎            ↔︎            ↔︎              |  ~\n~  0             600          960          1140         1440            ∞  ~\n~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n\n*/\n@mixin breakpoint($breakpointName) {\n  @if $breakpointName == veryNarrow {\n    @media (width < $b600) {@content;}\n  }\n  @else if $breakpointName == narrow {\n    @media (min-width: $b600) and (width < $b960) {@content;}\n  }\n  @else if $breakpointName == narrowAndUp {\n    @media (min-width: $b600) {@content;}\n  }\n  @else if $breakpointName == medium {\n    @media (min-width: $b960) and (width < $b1140) {@content;}\n  }\n  @else if $breakpointName == mediumAndUp {\n    @media (min-width: $b960) {@content;}\n  }\n  @else if $breakpointName == wide {\n    @media (min-width: $b1140) and (width < $b1440) {@content;}\n  }\n  @else if $breakpointName == wideAndUp {\n    @media (min-width: $b1140) {@content;}\n  }\n  @else if $breakpointName == veryWide {\n    @media (min-width: $b1440) {@content;}\n  }\n  @else {\n    @error 'Unknown brakpoint \"#{$breakpointName}\".';\n  }\n}\n"],"sourceRoot":""}]);
